<?xml version="1.0" encoding="utf-8" ?>
<RibbonItems xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="RibbonDefinitions.xsd">
  <TabCollections>
    <TabCollection Name="Mstn.Drawing">
      <TabRef Name="RhinoInside.Home" Priority="100000">
      </TabRef>
    </TabCollection>
    <TabCollection Name="ModelingTabs">
      <TabRef Name="RhinoInside.Home" Priority="120000">
      </TabRef>
    </TabCollection>
    <TabCollection Name="VisualizationTabs">
      <TabRef Name="RhinoInside.Home" Priority="100000">
      </TabRef>
    </TabCollection>
    <TabCollection Name="Mstn.TaskNavigation.Tabs">
      <TabRef Name="RhinoInside.Home" Priority="100000">
      </TabRef>
    </TabCollection>
  </TabCollections>
  <Tabs>
    <Tab Name="RhinoInside.Home" Priority="100">
      <Label>RhinoInside</Label>
      <TabItems>
        <GroupRef Name="RhinoInside.AttributeGroup" Priority="-10000">
          <GroupParams>
            <GroupLauncherAccessText>1</GroupLauncherAccessText>
            <AccessText>1</AccessText>
          </GroupParams>
        </GroupRef>
        <GroupRef Name="RhinoInside.ToolsGroup" Priority="-10000">
        </GroupRef>
      </TabItems>
    </Tab>
  </Tabs>
  <Groups>
    <Group GroupZone="Primary" Name="RhinoInside.AttributeGroup" PanelType="Collapsible" Priority="100">
      <Label>RhinoInside</Label>
      <Icon Type="NamedIcon">
        <NamedIcon>RibbonGroupPrimary</NamedIcon>
      </Icon>
      <GroupParams>
        <AccessText>S2</AccessText>
      </GroupParams>
      <GroupItems>
        <ButtonRef Name="RhinoInside.Hello" Priority="10000">
          <ButtonParams CollapseToSmall="WhenGroupIsSmall" Size="Large">
            <AccessText>SS</AccessText>
            <PopupAccessText>s</PopupAccessText>
          </ButtonParams>
        </ButtonRef>
        <ButtonRef Name="RhinoInside.Start" Priority="10000">
          <ButtonParams CollapseToSmall="WhenGroupIsSmall" Size="Large">
            <AccessText>SS</AccessText>
            <PopupAccessText>s</PopupAccessText>
          </ButtonParams>
        </ButtonRef>
        <ButtonRef Name="RhinoInside.RunGH" Priority="10000">
          <ButtonParams CollapseToSmall="WhenGroupIsSmall" Size="Large">
            <AccessText>SS</AccessText>
            <PopupAccessText>s</PopupAccessText>
          </ButtonParams>
        </ButtonRef>
        <ButtonRef Name="RhinoInside.WinFormsApp" Priority="10000">
          <ButtonParams CollapseToSmall="WhenGroupIsSmall" Size="Large">
            <AccessText>SS</AccessText>
            <PopupAccessText>s</PopupAccessText>
          </ButtonParams>
        </ButtonRef>
      </GroupItems>
    </Group>
    <Group GroupZone="Primary" Name="RhinoInside.ToolsGroup" PanelType="Collapsible" Priority="1000">
      <Label>Tools</Label>
      <Icon Type="NamedIcon">
        <NamedIcon>RibbonGroupPrimary</NamedIcon>
      </Icon>
      <GroupItems>
      </GroupItems>
    </Group>
  </Groups>
  <Buttons>
    <Button Name="RhinoInside.Hello" Priority="100">
      <Label>Hello</Label>
      <Description>Say hello</Description>
      <ButtonParams CollapseToSmall="WhenGroupIsSmall" Size="Large">
      </ButtonParams>
      <ButtonData>
        <CommandData Type="UserKeyinCommand">
          <UserKeyinCommand>
            <Keyin>RhinoInside Hello</Keyin>
            <Icon Type="NamedIcon">
              <NamedIcon>RibbonGroupPrimary</NamedIcon>
            </Icon>
          </UserKeyinCommand>
        </CommandData>
      </ButtonData>
    </Button>
    <Button Name="RhinoInside.Start" Priority="100">
      <Label>RhinoInside Test</Label>
      <Description>Launches RhinoInside</Description>
      <ButtonParams CollapseToSmall="WhenGroupIsSmall" Size="Large">
      </ButtonParams>
      <ButtonData>
        <CommandData Type="UserKeyinCommand">
          <UserKeyinCommand>
            <Keyin>RhinoInside Start</Keyin>
            <Icon Type="NamedIcon">
              <NamedIcon>RibbonGroupPrimary</NamedIcon>
            </Icon>
          </UserKeyinCommand>
        </CommandData>
      </ButtonData>
    </Button>
    <Button Name="RhinoInside.RunGH" Priority="100">
      <Label>RhinoInside Test RunGH</Label>
      <Description>Launches RhinoInside and run GH script</Description>
      <ButtonParams CollapseToSmall="WhenGroupIsSmall" Size="Large">
      </ButtonParams>
      <ButtonData>
        <CommandData Type="UserKeyinCommand">
          <UserKeyinCommand>
            <Keyin>RhinoInside RunGH</Keyin>
            <Icon Type="NamedIcon">
              <NamedIcon>RibbonGroupPrimary</NamedIcon>
            </Icon>
          </UserKeyinCommand>
        </CommandData>
      </ButtonData>
    </Button>
    <Button Name="RhinoInside.WinFormsApp" Priority="100">
      <Label>RhinoInside Test WinFormsApp</Label>
      <Description>Launches WinFormsApp</Description>
      <ButtonParams CollapseToSmall="WhenGroupIsSmall" Size="Large">
      </ButtonParams>
      <ButtonData>
        <CommandData Type="UserKeyinCommand">
          <UserKeyinCommand>
            <Keyin>RhinoInside WinFormsApp</Keyin>
            <Icon Type="NamedIcon">
              <NamedIcon>RibbonGroupPrimary</NamedIcon>
            </Icon>
          </UserKeyinCommand>
        </CommandData>
      </ButtonData>
    </Button>
  </Buttons>
</RibbonItems>
